		android:layout_height="match_parent"
		app:labelFields="Title,Subtitle,Date"
		app:layoutId="@layout/ddl_list_events"
		app:offlinePolicy="REMOTE_FIRST"
		app:recordSetId="79281"/>

</RelativeLayout>
